{
  "presets": ["flow", "env", "es2015", "preact", "stage-0", "stage-2"],
  "plugins": [
    ["transform-runtime", { "polyfill": false, "regenerator": true }],
    "syntax-flow",
    "transform-flow-strip-types",
    ["transform-react-jsx", { "pragma": "h" }],
    ["module-resolver", {
      "root": ["./src"]
    }]
  ]
}
