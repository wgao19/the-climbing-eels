{
  "presets": ["es2015", "stage-2", "react"],
  "plugins": [
    [
      "transform-runtime",
      {
        helpers: false,
        polyfill: false,
        regenerator: true
      }
    ],
    "syntax-flow",
    "transform-flow-strip-types",
    "transform-react-jsx",
    [
      "module-resolver",
      {
        "root": ["./src"]
      }
    ]
  ]
}
