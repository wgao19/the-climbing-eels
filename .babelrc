{
  "presets": ["flow", "env", "es2015", "stage-2", "preact"],
  "plugins": [
    ["transform-runtime", { "polyfill": false, "regenerator": true }],
    "syntax-flow",
    "transform-flow-strip-types",
    ["transform-react-jsx", { "pragma": "h" }],
    ["module-resolver", {
      "root": ["./src"]
    }]
  ]
}
